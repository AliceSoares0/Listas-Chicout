#include <stdio.h>

int main(void) {
  int i,a[10],b[10],c[10];
  printf("Numeros\n");
  for(i=0;i<10;i++){
    printf("A:");
    scanf("%i",&a[i]);
    printf("B:");
    scanf("%i",&b[i]);
    }
  printf("=======");
  for(i=0;i<10;i++){
    c[i]= a[i]-b[i]; 
  printf("C:%i\n",c[i]);
    }   
  return 0;
}
